	            sheet.addCell(S2_St_FirstName);
	            Label S2_St_LastName = new Label (2,2, "Mahayoden");
	            sheet.addCell(S2_St_LastName);
	            Label S2_St_Phone = new Label(3,2, "571-723-2113");
	            sheet.addCell(S2_St_Phone);
	            
	            //3rd Student
	            
	            Label S3_StudentID = new Label(0,3, "3256VA");
	            sheet.addCell(S3_StudentID );
	            Label S3_St_FirstName = new Label(1,3, "Hamidullah ");
	            sheet.addCell(S3_St_FirstName);
	            Label S3_St_LastName = new Label (2,3, "jan");
	            sheet.addCell(S3_St_LastName);
	            Label S3_St_Phone = new Label(3,3, "202-560-3669");
	            sheet.addCell(S3_St_Phone);
	            
	            //4rth Student 
	            Label S4_StudentID = new Label(0,4, "3257VA");
	            sheet.addCell(S4_StudentID );
	            Label S4_St_FirstName = new Label(1,4, "Muhammad Ehsan ");
	            sheet.addCell(S4_St_FirstName);
	            Label S4_St_LastName = new Label (2,4, "Zuhoor");
	            sheet.addCell(S4_St_LastName);
	            Label S4_St_Phone = new Label(3,4, "703-338-6906");
	            sheet.addCell(S4_St_Phone);
	            
	            //5th Student
	            
	            Label S5_StudentID = new Label(0,5, "3258VA");
	            sheet.addCell(S5_StudentID );
	            Label S5_St_FirstName = new Label(1,5, "Omid ");
	            sheet.addCell(S5_St_FirstName);
	            Label S5_St_LastName = new Label (2,5, "Mohmand");
	            sheet.addCell(S5_St_LastName);
	            Label S5_St_Phone = new Label(3,5, "202-840-1251");
	            sheet.addCell(S5_St_Phone);
	            
	            //6thStudent 
	            Label S6_StudentID = new Label(0,6, "3259VA");
	            sheet.addCell(S6_StudentID );
	            Label S6_St_FirstName = new Label(1,6, "Rafiullah ");
	            sheet.addCell(S6_St_FirstName);
	            Label S6_St_LastName = new Label (2,6, "Amerkhail");
	            sheet.addCell(S6_St_LastName);
	            Label S6_St_Phone = new Label(3,6, "703-989-2451");
	            sheet.addCell(S6_St_Phone);
	           
	            //7th Student 
	            Label S7_StudentID = new Label(0,7, "3260VA");
	            sheet.addCell(S7_StudentID );
	            Label S7_St_FirstName = new Label(1,7, "Ahmad Bashir");
	            sheet.addCell(S7_St_FirstName);
	            Label S7_St_LastName = new Label (2,7, "Jan");
	            sheet.addCell(S7_St_LastName);
	            Label S7_St_Phone = new Label(3,7, "571-315-5179");
	            sheet.addCell(S7_St_Phone);
	           
	            
	            
	            workbook.write();
	        
	        } catch (IOException e) {
	            e.printStackTrace();
	        }catch (RowsExceededException e) {
	        	e.printStackTrace();	
	        }catch (WriteException e) {
	        	e.printStackTrace();
	        }finally {
	        	//close the workbook
	        	workbook.close();
