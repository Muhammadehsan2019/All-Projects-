package TestNG_Practice;
import java.io.File;
import java.io.IOException;

import jxl.Workbook;
import jxl.write.Label;
import jxl.write.WritableCellFormat;
import jxl.write.WritableFont;
import jxl.write.WritableSheet;
import jxl.write.WritableWorkbook;
import jxl.write.WriteException;
import jxl.write.biff.RowsExceededException;

public class WriteToExcelCode {

	public static void main(String[] args) throws WriteException, IOException {
		// TODO Auto-generated method stub

		 WritableWorkbook workbook = null;
	        try {
	        	
	        	//initialize workbook
	            workbook = Workbook.createWorkbook(new File("E:\\1.PeopleinTech\\1.Selenuim\\All_files\\Working with excell\\TestSheet.xls"));
	            
	            //create sheet with name as MyNewData and index 0
	            
	            WritableSheet sheet = workbook.createSheet("QA_Testers", 0);
	            
	            //create font styles for header cells
	            
	            WritableFont headerCellFont = new WritableFont(WritableFont.TIMES, 12,WritableFont.BOLD);
	            
	            //create format for header cells
	            WritableCellFormat headerCellFormat = new WritableCellFormat(headerCellFont);
	            
	            
	            //create header cells
	            
	            Label label = new Label(0, 0, "Student_ID", headerCellFormat);
	            sheet.addCell(label);
	            Label label1 = new Label(1, 0, "StFirstName",headerCellFormat);
	            sheet.addCell(label1);
	            Label label2 = new Label(2, 0, "StLastName",headerCellFormat);
	            sheet.addCell(label2);
	            Label label3 = new Label (3,0,"StPhoneNumber", headerCellFormat);
	            sheet.addCell(label3);
	            // setting width of colums 
	            sheet.setColumnView(0, 20);
	            sheet.setColumnView(1, 20);
	            sheet.setColumnView(2, 20);
	            sheet.setColumnView(3, 20);
	            
	            
	            //create cell contents for Student1
	            Label StudentID = new Label(0,1, "3254VA");
	            sheet.addCell(StudentID);
	            Label St_FirstName = new Label(1,1, "Hedayatullah");
	            sheet.addCell(St_FirstName);
	            Label St_LastName = new Label (2,1, "Aka");
